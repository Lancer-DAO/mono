{
  "name": "extension-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "app": "yarn build && yarn export",
    "background": "webpack --config background.webpack.config.js",
    "build": "rm -rf ./build && NODE_ENV=production next build",
    "export": "next export",
    "compile": "yarn background && yarn content",
    "content": "webpack --config content.webpack.config.js",
    "clean": "rm -rf extension && mkdir extension",
    "copy": "mv out/_next out/next && mv out/popup.html extension && mv out/modal.html extension && cp -r out/next/ extension/ && rm -rf out && cp -r public/ extension/ && cp -r .next/server extension && cp manifest.json extension",
    "dev": "next dev",
    "deploy": "yarn build && gcloud app deploy",
    "modal": "yarn app && yarn copy && yarn replace",
    "ext": "yarn clean && yarn modal && yarn compile",
    "server": "concurrently \"tsc --watch server\" \"nodemon -q dist/index.js\"",
    "start": "next start -p 8080",
    "tsc": "tsc server/server.ts",
    "replace": "sed -i 's+/_next+/next+g' extension/next/static/chunks/*.js && sed -i 's+/_next+/next+g' extension/next/static/chunks/pages/*.js && sed -i 's+/_next+/next+g' extension/next/static/css/*.css && sed -i 's+/_next+/next+g' extension/server/chunks/*.js && sed -i 's+/_next+/next+g' extension/server/pages/*.html && sed -i 's+/_next+/next+g' extension/server/pages/*.js && sed -i 's+/_next+/next+g' extension/*.html"
  },
  "dependencies": {
    "@next/font": "13.1.2",
    "@solana/spl-token": "0.3.4",
    "@solana/wallet-adapter-base": "0.9.19",
    "@solana/wallet-adapter-react": "0.15.25",
    "@solana/wallet-adapter-react-ui": "0.9.23",
    "@solana/wallet-adapter-wallets": "0.19.8",
    "@solana/web3.js": "^1.50.1",
    "@toruslabs/openlogin": "^2.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@web3auth/base": "^1.1.4",
    "@web3auth/core": "^1.1.4",
    "@web3auth/openlogin-adapter": "^1.1.4",
    "axios": "1.2.3",
    "buffer": "6.0.3",
    "classnames": "2.3.2",
    "cors": "2.8.5",
    "concurrently": "7.6.0",
    "cryptocurrency-icons": "0.18.1",
    "dotenv": "^16.0.1",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.2",
    "events": "^3.3.0",
    "express": "4.18.2",
    "lodash": "4.17.21",
    "next": "13.1.2",
    "nodemon": "2.0.20",
    "octokit": "2.0.11",
    "process": "0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "2.0.10",
    "react-loader-spinner": "^5.1.7-beta.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-switch": "^7.0.0",
    "request": "2.88.2",
    "sass": "1.56.2",
    "sass-loader": "13.2.0",
    "start": "^5.1.0",
    "tsup": "6.5.0",
    "ts-loader": "9.4.2",
    "typescript": "4.9.4",
    "util": "^0.12.4",
    "web-vitals": "^3.0.3",
    "web3": "^1.7.5"
  },
  "devDependencies": {
    "@types/chrome": "0.0.208",
    "@types/express": "^4.17.16",
