{
    "compilerOptions": {
      "target": "ES2016",
      "useDefineForClassFields": true,
      "lib": [
        "DOM",
        "DOM.Iterable",
        "ESNext"
      ],
      "allowJs": false,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": false,
      "forceConsistentCasingInFileNames": true,
      "module": "CommonJS",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react-jsx",
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "../*"
        ],
        "@/constants": [
          "../app/src/constants/index.ts"
        ],
        "@/styles": [
          "../src/styles"
        ],
        "@/server": [
          "../server"
        ],
        "@/types": [
          "../app/src/types/index.ts"
        ],
        "@/utils": [
          "../src/utils/index.ts"
        ]
      },
      "outDir": "./dist",
      "noEmit": false,
      "incremental": true
    },
    "include": [
      "next-env.d.ts",
      "**/*.ts",
      "**/*.tsx"
    ],
    "exclude": [
      "node_modules"
    ]
  }
